SELECT INF_INET.ORDERS WITH (MONTH.PERIOD LIKE "...2021..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...Q1..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...Q3..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...Q2..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...R1..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...R2..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...R3..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...RU..." AND EVAL "TRANS('INET.CAMPAIGNS',SUBR('-OCONVS',CAMPAIGN.ID,'MD00'),'STATUS.DESC','X')" UNLIKE "...DE..." AND LINE.CANCEL.STATUS.ID UNLIKE "...DE...")
LIST INF_INET.ORDERS BY CURRENT.REP.IDS EVAL "CHAR(244):CHAR(171):CONVERT(CHAR(253),CHAR(250),CONVERT(CHAR(252),CHAR(249),CONVERT(CHAR(251),CHAR(248),CONVERT(CHAR(32),CHAR(7),CONVERT(CHAR(4),CHAR(245),CONVERT(CHAR(13),CHAR(247),CONVERT(CHAR(10),CHAR(246),SUBR('ESC.AT.V5',SUBR('-OCONVS',MONTH.ACTUAL.AMT,'MD22')):CHAR(166):SUBR('ESC.AT.V5',CURRENT.REP.IDS):CHAR(166):SUBR('ESC.AT.V5',MONTH.PERIOD):CHAR(166):SUBR('ESC.AT.V5',SUBR('-OCONVS',MONTH.ACTUAL.COMM.AMT,'MD22')):CHAR(166):@ID:CHAR(166):SUBR('ESC.AT.V5',SUBR('-OCONVS',MONTH.EST.AMT,'MD22')):CHAR(166):SUBR('ESC.AT.V5',SUBR('-OCONVS',MONTH.START.DATE,'D4/')):CHAR(166):WEB.SITE.ID:CHAR(166):SUBR('ESC.AT.V5',BRAND.REPS)))))))):CHAR(187)" CNV "" FMT "80L" ID.SUP COUNT.SUP COL.SPCS 0 HDR.SUP COL.SUP REQUIRE.SELECT
